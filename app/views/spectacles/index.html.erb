<h1>Spectacles#:index</h1>
<p>Find me in app/views/spectacles/:index.html.erb</p>
  <p><%= link_to "new", new_spectacle_path %></p>
  <% @spectacles.each do | spectacle | %>
  <div class="card p-4 mt-2 mb-2">
    <h4><%= spectacle.title %></h4>

    <p><%= spectacle.origin %></p>
    <p><%= spectacle.public %></p>
    <%= link_to "Detail", spectacle %>
    <%= button_to "Delete", spectacle, method: :delete %>
    <%= link_to "Edit", edit_spectacle_path(spectacle) %>


  </div>
  <% end %>
